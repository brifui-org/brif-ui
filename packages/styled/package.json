{
  "name": "@brifui/styled",
  "version": "0.0.28",
  "description": "Styled system for Brif UI.",
  "sideEffects": false,
  "type": "module",
  "keywords": [
    "styled",
    "theming",
    "ui"
  ],
  "files": [
    "dist",
    "distcjs"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./css": {
      "types": "./dist/css/index.d.ts",
      "import": "./dist/css/index.mjs",
      "require": "./distcjs/css/index.cjs"
    },
    "./tokens": {
      "types": "./dist/tokens/index.d.ts",
      "import": "./dist/tokens/index.mjs",
      "require": "./distcjs/tokens/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./distcjs/types/index.cjs"
    },
    "./patterns": {
      "types": "./dist/patterns/index.d.ts",
      "import": "./dist/patterns/index.mjs",
      "require": "./distcjs/patterns/index.cjs"
    },
    "./recipes": {
      "types": "./dist/recipes/index.d.ts",
      "import": "./dist/recipes/index.mjs",
      "require": "./distcjs/recipes/index.cjs"
    },
    "./helpers": {
      "import": "./dist/helpers.mjs",
      "require": "./distcjs/helpers.cjs"
    }
  },
  "scripts": {
    "build": "rslib build",
    "codegen": "panda codegen",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ."
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "devDependencies": {
    "@brifui/eslint-config": "^0.0.3",
    "@brifui/types": "^0.0.14",
    "@brifui/typescript-config": "^0.0.3",
    "@rslib/core": "^0.5.5",
    "delay-cli": "^2.0.0",
    "eslint": "^9.25.1",
    "typescript": "5.8.3",
    "vitest": "^2.1.9"
  },
  "dependencies": {
    "@brifui/theme": "^0.0.26",
    "@pandacss/dev": "^0.53.4"
  }
}
