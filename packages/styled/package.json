{
  "name": "@brifui/styled",
  "version": "0.0.19",
  "description": "Styled system for Brif UI.",
  "sideEffects": false,
  "type": "module",
  "keywords": [
    "styled",
    "theming",
    "ui"
  ],
  "files": [
    "dist"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./css": {
      "types": "./dist/css/index.d.ts",
      "import": "./dist/css/index.mjs"
    },
    "./tokens": {
      "types": "./dist/tokens/index.d.ts",
      "import": "./dist/tokens/index.mjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs"
    },
    "./patterns": {
      "types": "./dist/patterns/index.d.ts",
      "import": "./dist/patterns/index.mjs"
    },
    "./helpers": {
      "import": "./dist/helpers.mjs"
    }
  },
  "scripts": {
    "codegen": "panda codegen",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ."
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "peerDependencies": {
    "@pandacss/dev": "^0.53.1"
  },
  "devDependencies": {
    "@brifui/eslint-config": "^0.0.3",
    "@brifui/types": "^0.0.12",
    "@brifui/typescript-config": "^0.0.3",
    "eslint": "^9.15.0",
    "typescript": "5.5.4",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@brifui/theme": "^0.0.18",
    "@pandacss/dev": "^0.53.1"
  }
}
