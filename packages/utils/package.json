{
  "name": "@brifui/utils",
  "version": "0.0.19",
  "description": "Brif UI components - utils library",
  "sideEffects": false,
  "type": "module",
  "keywords": [
    "utils",
    "ui"
  ],
  "files": [
    "dist",
    "src"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src --dts",
    "dev": "tsup src --watch",
    "prepack": "clean-package",
    "postpack": "clean-package restore",
    "lint": "eslint ."
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "dependencies": {
    "@brifui/styled": "^0.0.24",
    "@brifui/types": "^0.0.13",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@brifui/eslint-config": "^0.0.3",
    "@brifui/typescript-config": "^0.0.3",
    "@microsoft/api-extractor": "^7.49.1",
    "@types/react": "^19.0.5",
    "@types/react-dom": "^19.0.3",
    "eslint": "^9.15.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "5.5.4",
    "vitest": "^2.1.8"
  },
  "tsup": {
    "minify": false,
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ]
  }
}
