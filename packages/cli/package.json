{
  "name": "@brifui/cli",
  "version": "0.0.27",
  "description": "CLI tool for Brif UI.",
  "type": "module",
  "sideEffects": false,
  "keywords": [
    "theme",
    "theming",
    "ui"
  ],
  "files": [
    "bin",
    "dist",
    "src"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "brif": "./bin/bin.js",
    "brifui": "./bin/bin.js"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsup src --dts",
    "prepack": "clean-package",
    "postpack": "clean-package restore",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@brifui/eslint-config": "^0.0.4",
    "@brifui/node": "^0.0.5",
    "@brifui/typescript-config": "^0.0.4",
    "@types/node": "^22.15.31",
    "cac": "^6.7.14",
    "clean-package": "^2.2.0",
    "eslint": "^9.29.0",
    "typescript": "5.8.3",
    "update-notifier": "^7.3.1",
    "vitest": "^2.1.9"
  },
  "dependencies": {
    "@clack/prompts": "0.9.1",
    "@pandacss/node": "^0.54.0",
    "@rslib/core": "^0.5.5",
    "bundle-n-require": "^1.1.2",
    "chalk": "4.1.2",
    "effect": "^3.16.7",
    "escalade": "^3.2.0",
    "fast-glob": "^3.3.3",
    "find-up": "^7.0.0",
    "minimatch": "^10.0.3"
  },
  "tsup": {
    "splitting": false,
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ],
    "shims": true
  }
}
